		<category><![CDATA[classe C]]></category>
		<category><![CDATA[classe DE]]></category>
		<category><![CDATA[classes sociais]]></category>
		<category><![CDATA[data popular]]></category>
		<category><![CDATA[economia]]></category>
		<category><![CDATA[ferrari]]></category>
		<category><![CDATA[inclusão digital]]></category>
		<category><![CDATA[Internet]]></category>
		<category><![CDATA[pirâmide social]]></category>
		<category><![CDATA[públicos]]></category>
		<category><![CDATA[smartphones]]></category>
