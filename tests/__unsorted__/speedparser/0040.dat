		<pubDate>Thu, 15 Sep 2011 06:45:08 +0000</pubDate>
		<dc:creator>Elsa</dc:creator>
				<category><![CDATA[Food]]></category>
		<category><![CDATA[bread]]></category>
		<category><![CDATA[recipes]]></category>
