				}
				
				if(haut_mess_form == true) {
					$("#message_error").css("display","block");
					$("#message_error").html(message);
				}
			}
			
			$("#formulaire_contact").animate({ backgroundColor: "#e5e5e5" }, 800);
			return valid;
