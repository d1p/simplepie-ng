			}if($("#cispm_input_reponse").val() != "2" && $("#cispm_input_reponse").val() != "") {
				$("#cispm_span_question").css("color","red");$('#cispm_input_reponse').next().text('Ce n\'est pas la bonne réponse');$('#cispm_input_reponse').next().css('text-align','left');message_error_question = "Ce n'est pas la bonne réponse";
				valid = false;
