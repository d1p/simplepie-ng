				tab_error[4] = "";
			}if($("#message_input").val() == "") {
				$("#message_input").css("background-color","#FFFF99");
				$("#span_message").css("color","#FF0000");$('#message_input').next().text('Ce champ est obligatoire');$('#message_input').next().css('text-align','left');tab_error[5] = "Vos demandes : <br/>";
				valid = false;
