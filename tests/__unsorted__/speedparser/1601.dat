		<pubDate>Wed, 31 Aug 2011 11:39:34 +0000</pubDate>
		<dc:creator>daniela@colorfulbeauty</dc:creator>
				<category><![CDATA[articles]]></category>
		<category><![CDATA[color me awful]]></category>
		<category><![CDATA[color me pretty]]></category>
		<category><![CDATA[how to]]></category>
