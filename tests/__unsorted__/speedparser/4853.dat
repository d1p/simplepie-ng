<media:content height='20' isDefault='false' medium='image' type='image/jpeg' url='http://o2.aolcdn.com/dims-shared/dims3/PATCH/thumbnail/20x32/crop/20x20+0+6/http://hss-prod.hss.aol.com/hss/storage/patch/6828f82270666c1f628e41400459abf0' width='20'></media:content>
