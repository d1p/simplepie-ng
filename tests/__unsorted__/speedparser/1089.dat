      <item>
         <title>Refrigerator Reorganization</title>
         <link>http://feedproxy.google.com/~r/evanwilder/photoblog/~3/16oNsbXo5CA/</link>
         <description>&lt;p&gt;&lt;a rel=&quot;nofollow&quot; target=&quot;_blank&quot; href=&quot;http://photoblog.evanwilder.com/?p=367&quot;&gt;.&lt;/a&gt;&lt;br /&gt;
