		<pubDate>Fri, 22 Apr 2011 13:09:24 +0000</pubDate>
		<dc:creator>Louise</dc:creator>
				<category><![CDATA[OOTD]]></category>
		<category><![CDATA[best & less]]></category>
		<category><![CDATA[big w]]></category>
		<category><![CDATA[eva morning]]></category>
