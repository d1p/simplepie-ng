<script type="text/javascript">
	$(function() {
		$(window).nq_browsers({
			url			: 'http://www.newquest.fr/blog/wp-content/themes/newquest/browsers.html',
			ie6Access 	: false
		});		   

        var first_column = $("ol.commentlist").parent();
        var max_height = first_column.height();
        
        var content_column = $("div#article-com");

        $("div.tmp_data").append( $("ol.commentlist").children() );
        
        var cur_ol = first_column.find("ol").first();
        var curent_height = $("div#comment_form").innerHeight();
        var el_height;
        
        $("div.tmp_data li").each(function(i)
        {
            el_height = $(this).innerHeight();
            if( (curent_height + el_height) > (max_height - 100) && (el_height < max_height) )
            {
                content_column.append("<div class=\'column\'><ol class=\'commentlist\'></ol></div>");
                cur_ol          = content_column.find("ol.commentlist").last();
                curent_height   = 0;
            }
            
            cur_ol.append( $(this) );
            curent_height += $(this).innerHeight();
            
        });        
        
        $("div.tmp_data").remove();
        $("div#article-com").children().last().attr('class', 'column last');
        load_jScrollPane( window.location.hash == '#new-comment'? $('.column').length * $('.column').width() : null );
    });  
</script>


