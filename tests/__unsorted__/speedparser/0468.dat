    <author>
        <name>Jujuqui</name>
        <uri>http://playstop.net</uri>
    </author>
    
        <category term="Coisas Legais" scheme="http://www.sixapart.com/ns/types#category" />
    
        <category term="Esquisitices Peculiares" scheme="http://www.sixapart.com/ns/types#category" />
    
    <category term="antiguidades" label="Antiguidades" scheme="http://www.sixapart.com/ns/types#tag" />
    <category term="blog" label="Blog" scheme="http://www.sixapart.com/ns/types#tag" />
    <category term="esquisitices" label="Esquisitices" scheme="http://www.sixapart.com/ns/types#tag" />
    <category term="futilidades" label="Futilidades" scheme="http://www.sixapart.com/ns/types#tag" />
    <category term="ilustração" label="Ilustração" scheme="http://www.sixapart.com/ns/types#tag" />
