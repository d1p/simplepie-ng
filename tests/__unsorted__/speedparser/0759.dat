		<dc:creator>Julien Guillot</dc:creator>
				<category><![CDATA[Applications]]></category>
		<category><![CDATA[iOS]]></category>
		<category><![CDATA[iPad]]></category>
		<category><![CDATA[Matériel]]></category>
