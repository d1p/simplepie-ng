  <content type="html">&lt;p&gt;Személyesen Sinyától nektek &lt;/p&gt; &lt;p&gt;&lt;object height=&quot;298&quot; width=&quot;460&quot; codebase=&quot;http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,40,0&quot; classid=&quot;clsid:d27cdb6e-ae6d-11cf-96b8-444553540000&quot;&gt;&lt;param value=&quot;high&quot; name=&quot;quality&quot;&gt;&lt;/param&gt;&lt;param value=&quot;always&quot; name=&quot;allowScriptAccess&quot;&gt;&lt;/param&gt;&lt;param value=&quot;true&quot; name=&quot;allowFullScreen&quot;&gt;&lt;/param&gt;&lt;param value=&quot;vID=2e9e0c639b&amp;autostart=false&quot; name=&quot;flashvars&quot;&gt;&lt;/param&gt;&lt;param value=&quot;guPlayer-2e9e0c639b&quot; name=&quot;name&quot;&gt;&lt;/param&gt;&lt;param value=&quot;http://files.indavideo.hu/player/gup.swf&quot; name=&quot;src&quot;&gt;&lt;/param&gt;&lt;param value=&quot;#666666&quot; name=&quot;bgcolor&quot;&gt;&lt;/param&gt;&lt;embed height=&quot;298&quot; align=&quot;middle&quot; width=&quot;460&quot; pluginspage=&quot;http://www.macromedia.com/go/getflashplayer&quot; type=&quot;application/x-shockwave-flash&quot; allowfullscreen=&quot;true&quot; allowscriptaccess=&quot;always&quot; name=&quot;guPlayer-2e9e0c639b&quot; bgcolor=&quot;#666666&quot; quality=&quot;high&quot; src=&quot;http://files.indavideo.hu/player/gup.swf&quot; flashvars=&quot;vID=2e9e0c639b&amp;autostart=false&quot;&gt;&lt;/embed&gt;&lt;/object&gt;&lt;/p&gt;&lt;p&gt;A &lt;a href=&quot;http://kerekagy.blog.hu/2010/01/11/biciklin_nem_lehet_fazni&quot; target=&quot;_blank&quot;&gt;Kerékagyról&lt;/a&gt;&lt;/p&gt;
