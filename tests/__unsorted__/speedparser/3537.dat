<script>
$j(document).ready(function() {
	$j(window).bind('facebook.user.session', user);
	
	function user() {
		if (FBButtonClick) {
			if ($j.facebook.user.uid != ''){
				$j.ajax({
					url: '/NewaNobii/aNobiiHome/live/account/facebook-connect?facebookUid='+$j.facebook.user.uid,
					success: function(data) {
						if (data == '1') {
							location.href = '/home';     
						} else {
							location.href = '/sign_up';
						}
					}
				});
			}
		}
	}
	
	$j.facebook();
	$j('#login_facebook').facebook();
	$j('#login_facebook').facebook('show');
});

</script>
