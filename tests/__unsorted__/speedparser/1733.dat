&lt;dt&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFFFFF&quot;&gt;&lt;a href=&quot;http://www.eltridente.es/novedades/889-humo-sapiens-cd-en-los-bolsillos.html&quot; target=&quot;_blank&quot;&gt;&lt;font color=&quot;#FFFFFF&quot; size=&quot;2&quot;&gt;&lt;img hspace=&quot;0&quot; src=&quot;http://www.produccionesmalditas.com/boletines/images/digital/portada_HUMO_SAPIENS_web.jpg&quot; border=&quot;0&quot;/&gt;&lt;/font&gt;&lt;/a&gt;&lt;/font&gt;&lt;/dt&gt;
&lt;dt&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFFFFF&quot;&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFFFFF&quot;&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFFFFF&quot; size=&quot;3&quot;&gt;______________________________&lt;/font&gt;______________________________&lt;/font&gt;&lt;/font&gt;&lt;/dt&gt;
&lt;dt&gt;
 &lt;br /&gt;
&lt;br /&gt;
&lt;/dt&gt;
&lt;dt&gt;&lt;font size=&quot;2&quot;&gt;&lt;font face=&quot;Arial&quot;&gt;&lt;b&gt;&lt;font color=&quot;#FFCC00&quot;&gt;HUMO SAPIENS&lt;/font&gt;&lt;/b&gt; &lt;font color=&quot;#FFFFFF&quot;&gt;en Internet:&lt;/font&gt;&lt;/font&gt;&lt;/font&gt;&lt;/dt&gt;
&lt;dt&gt;
&lt;font size=&quot;2&quot;&gt;&lt;b&gt;&lt;a href=&quot;http://www.myspace.com/humosapiens&quot; target=&quot;_blank&quot;&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFCC00&quot;&gt;Myspace&lt;/font&gt;&lt;/a&gt;&lt;font color=&quot;#FFCC00&quot;&gt;&lt;font face=&quot;Arial&quot;&gt;  &lt;/font&gt;&lt;font face=&quot;Arial&quot;&gt;·  &lt;/font&gt;&lt;/font&gt;&lt;a href=&quot;http://www.humosapiens.es/&quot; target=&quot;_blank&quot;&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFCC00&quot;&gt;WEB OFICIAL&lt;/font&gt;&lt;/a&gt;&lt;font color=&quot;#FFCC00&quot;&gt;&lt;font face=&quot;Arial&quot;&gt;  &lt;/font&gt;&lt;font face=&quot;Arial&quot;&gt;  ·  &lt;/font&gt;&lt;/font&gt;&lt;a href=&quot;http://www.facebook.com/HumoSapiens&quot; target=&quot;_blank&quot;&gt;&lt;font face=&quot;Arial&quot; color=&quot;#FFCC00&quot;&gt;Facebook&lt;/font&gt;&lt;/a&gt;&lt;/b&gt;&lt;/font&gt;&lt;br /&gt;
