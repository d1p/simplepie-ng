&lt;dt&gt;&lt;font face=&quot;Verdana&quot; color=&quot;#FFFFFF&quot; size=&quot;2&quot;&gt;&lt;font color=&quot;#FFCC00&quot;&gt;&lt;b&gt;&lt;font color=&quot;#FFFFFF&quot;&gt;Nuevo disco &lt;font color=&quot;#FFCC99&quot;&gt;&quot;En Los Bolsillos&quot;&lt;/font&gt; a la venta en&lt;/font&gt; &lt;/b&gt;&lt;a href=&quot;http://www.eltridente.es/novedades/889-humo-sapiens-cd-en-los-bolsillos.html&quot; target=&quot;_blank&quot;&gt;&lt;font color=&quot;#339966&quot;&gt;&lt;b&gt;El Tridente&lt;/b&gt;&lt;/font&gt;&lt;/a&gt;&lt;/font&gt;&lt;font color=&quot;#FFFFFF&quot;&gt;!!&lt;/font&gt;&lt;/font&gt;&lt;/dt&gt;
