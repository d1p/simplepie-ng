&lt;span class=&quot;codenf&quot;&gt;poly_epr1_test&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;()&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;&lt;span class=&quot;codeno&quot;&gt;?assert&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;codes&quot;&gt;&amp;quot;3x^3+4x^2-3&amp;quot;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;:&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;coden&quot;&gt;poly_epr&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;([&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;3&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;4&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;3&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;])).&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;

&lt;span class=&quot;codenf&quot;&gt;poly_first_negative_test&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;()&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;&lt;span class=&quot;codeno&quot;&gt;?assert&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;codes&quot;&gt;&amp;quot;-3x^4-4x^3+x^2+6&amp;quot;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;:&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;coden&quot;&gt;poly_epr&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;([&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;3&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;4&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;1&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;6&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;])).&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;

&lt;span class=&quot;codenf&quot;&gt;poly_simple_test&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;()&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;&lt;span class=&quot;codeno&quot;&gt;?assert&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;codes&quot;&gt;&amp;quot;x^2+2&amp;quot;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;:&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;coden&quot;&gt;poly_epr&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;([&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;1&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;2&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;])).&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;

&lt;span class=&quot;codenf&quot;&gt;poly_first_minus_one_test&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;()&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
