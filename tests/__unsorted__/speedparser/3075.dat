&lt;span class=&quot;code&quot;&gt;    &lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;_&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
