&lt;span class=&quot;codenf&quot;&gt;poly_all_zera_test&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;()&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;&lt;span class=&quot;codeno&quot;&gt;?assert&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;codes&quot;&gt;&amp;quot;0&amp;quot;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;:&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;coden&quot;&gt;poly_epr&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;([&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;0&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;])).&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;

&lt;span class=&quot;codenf&quot;&gt;poly_test_error_test&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;()&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;{&lt;/span&gt;&lt;span class=&quot;coden&quot;&gt;error&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;,&lt;/span&gt;&lt;span class=&quot;codenv&quot;&gt;Msg&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;}&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;coden&quot;&gt;poly_epr&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;([&lt;/span&gt;&lt;span class=&quot;codemi&quot;&gt;1&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;]),&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
&lt;span class=&quot;code&quot;&gt;  &lt;/span&gt;&lt;span class=&quot;codeno&quot;&gt;?assert&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;codes&quot;&gt;&amp;quot;Need at least 2 coefficients&amp;quot;&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;:&lt;/span&gt;&lt;span class=&quot;codeo&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;code&quot;&gt; &lt;/span&gt;&lt;span class=&quot;codenv&quot;&gt;Msg&lt;/span&gt;&lt;span class=&quot;codep&quot;&gt;).&lt;/span&gt;&lt;span class=&quot;code&quot;&gt;&lt;/span&gt;
