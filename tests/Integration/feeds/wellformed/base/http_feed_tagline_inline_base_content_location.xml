<!--
Header:      Content-Location: http://example.com/
Description: feed tagline relative to Content-Location header
Expect:      not bozo and feed['tagline'] == '<div><a href="http://example.com/relative/uri">click here</a></div>'
-->
<feed xmlns="http://www.w3.org/2005/Atom">
<tagline type="xhtml"><div xmlns="http://www.w3.org/1999/xhtml"><a href="/relative/uri">click here</a></div></tagline>
</feed>
