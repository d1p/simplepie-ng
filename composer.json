{
    "name": "simplepie/simplepie-ng",
    "description": "A modern feed parsing library for PHP.",
    "keywords": ["php", "rss", "atom", "json", "feed", "xml", "dublincore", "rdf"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ryan Parman",
            "homepage": "http://ryanparman.com"
        }
    ],
    "support": {
        "issues": "https://github.com/simplepie/simplepie-ng/issues"
    },
    "require": {
        "php": ">=7.1",
        "psr/log": "^1.0",
        "guzzlehttp/promises": "^1.3",
        "guzzlehttp/guzzle": "^6.2",
        "cache/cache": "^0.4.0",
        "samburns/pimple3-containerinterop": "^1.3",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "SimplePie\\": "src/",
            "SimplePie\\Tests\\": "tests/"
        }
    }
}
